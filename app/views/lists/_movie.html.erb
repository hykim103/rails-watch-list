<% @list.movies.each do |movie| %>
  <img src="<%= movie.poster_url %>" alt="<%= movie.title %> poster">
  <p><%= movie.title %></p>
  <p><%= movie.rating %></p>
  <p><%= movie.overview %></p>
  <p><%= movie.bookmarks.first.comment %></p>
  <p><%= link_to "delete", bookmark_path(movie.bookmarks.first), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}%></p>
<% end %>
